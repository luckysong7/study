<%@ page contentType="text/html; charset=UTF-8"%>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
<title>Insert title here</title>
</head>
<body bgcolor="#F5F6F7">
	<div align="center">
		<img alt="그림 없음" src="./image/naver123.png" align="middle">
	</div>
	<table border="0" align="center" cellpadding="0" cellspacing="0"
		height="300" width="400">
		<tr height="40">
			<td colspan="3"><b>아이디</b></td>
		</tr>
		<tr height="40">
			<td colspan="3"><input type="text" style="width: 100%; height: 100%; text-align: right" value ="@naver.com" /></td>
			
		</tr>
		<tr height="40">
			<td colspan="3"><b>비밀번호</b></td>
		</tr>
		<tr height="40">
			<td colspan="3"><input type="password"
				style="width: 100%; height: 100%" /></td>
		</tr>
		<tr height="40">
			<td colspan="3"><b>비밀번호 재확인</b></td>
		</tr>
		<tr height="40">
			<td width="100%" colspan="3"><input type="password"
				style="width: 100%; height: 100%" /></td>
		</tr>
		<tr height="40">
			<td colspan="3"><b>이름</b></td>
		</tr>
		<tr height="40">
			<td colspan="3"><input type="text"
				style="width: 100%; height: 100%" /></td>
		</tr>
		<tr height="40">
			<td colspan="3"><b>생년월일</b></td>
		</tr>
		<tr height="40">
			<td><input type="text" size="4" value ="년(4자)" style="width:90%; height:100%"/></td>
			<td><select style="width: 100%; height: 100%"><option style="width: 100%; height: 100%">월</option>
					<option>1</option>
					<option>2</option>
					<option>3</option>
					<option>4</option>
					<option>5</option>
					<option>6</option>
					<option>7</option>
					<option>8</option>
					<option>9</option>
					<option>10</option>
					<option>11</option>
					<option>12</option>
			</select></td>
			<td><input type="text" style="width: 100%; height: 100%" value="일" /></td>
		</tr>
		<tr height="40">
			<td colspan="3"><b>성별</b></td>
		</tr>
		<tr height="40">
			<td colspan="2" width="150"><input type="button" value="남자"
				style="width: 100%; height: 100%"></td>
			<td><input type="button" value="여자"
				style="width: 100%; height: 100%"></td>
		</tr>

		<tr height="40">
			<td colspan="3"><b>본인 확인 이메일</b></td>
		</tr>
		<tr height="40">
			<td colspan="3"><input type="text"
				style="width: 100%; height: 100%" value = "선택입력"/></td>
		</tr>
		<tr height="40">
			<td colspan="3"><b>휴대전화</b></td>
		</tr>
		<tr height="40">
			<td colspan="3"><select style="width: 100%; height: 100%">
					<option>대한민국 +82</option>
					<option>벨기에 +32</option>
					<option>영국 +44</option>
					<option>홍콩 +852</option>
			</select></td>
		</tr>
		<tr height="40">
			<td colspan="2"><input type="text"
				style="width: 100%; height: 100%" value ="전화번호 입력" /></td>
			<td><input type="button" value="인증번호 받기"
				style="width: 100%; height: 100%; background-color: #08A600; color: white; text-align: center; font-size: 13pt;" /></td>
		</tr>
		<tr height="40">
			<td colspan="3"><input type="text" value="인증번호 입력하세요"
				style="width: 99%; height: 100%" /></td>
		</tr>
		<tr height="20"></tr>
		<tr height="40">
			<td colspan="3"><input type="button" value="가입하기"
				style="width: 100%; height: 100%; background-color: #08A600; color: white; text-align: center; font-size: 13pt;" /></td>
		</tr>
	</table>



</body>
</html>